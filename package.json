{
  "name": "body-tracker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "emulator:start": "firebase emulators:start",
    "start:dev": "nx run-many --target=serve --projects=diet",
    "dev": "concurrently --kill-others \"yarn run emulator:start\" \"wait-on http-get://localhost:4000 && yarn run start:dev\"",
    "build": "nx run-many --target=build --parallel",
    "test": "nx run-many --target=test --exclude=workspace --parallel",
    "lint": "nx run-many --target=lint --parallel --exclude=workspace",
    "type-check": "nx run-many --target=type-check --parallel --exclude=workspace",
    "prepare": "husky install",
    "ui:dev": "nx run ui:storybook",
    "ui:build": "nx run ui:build-storybook",
    "ui-diet:dev": "nx run ui-diet:storybook",
    "ui-diet:build": "nx run ui-diet:build-storybook",
    "oda-scrape": "nx run oda-scraper:scrape",
    "oda-search": "nx run oda-scraper:search",
    "oda-repl": "ts-node -r tsconfig-paths/register --project ./libs/oda-scraper/tsconfig.json ./libs/oda-scraper/src/scripts/searchREPL.ts",
    "foodtable-normalize": "nx run foodtable-scraper:normalize",
    "foodtable-scrape": "nx run foodtable-scraper:scrape",
    "foodtable-search": "nx run foodtable-scraper:search",
    "foodtable-repl": "ts-node -r tsconfig-paths/register --project ./libs/foodtable-scraper/tsconfig.json ./libs/foodtable-scraper/src/scripts/searchREPL.ts"
  },
  "private": true,
